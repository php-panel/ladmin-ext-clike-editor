{
    "name": "php-panel/ladmin-ext-clike-editor",
    "description": "Several c-lick language editor extensions for laravel-admin based on code-mirror",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "extension",
        "code-mirror",
        "editor",
        "clang",
        "cpp",
        "java",
        "objectivec",
        "scala",
        "kotlin",
        "Ceylon"
    ],
    "homepage": "https://github.com/php-panel/ladmin-ext-clike-editor",
    "license": "MIT",
    "authors": [
        {
            "name": "leeqvip",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "php-panel/laravel-admin": "^3.0.3",
        "php-panel/ladmin-ext-code-mirror": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.0"
    },
    "autoload": {
        "psr-4": {
            "Ladmin\\ClikeEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladmin\\ClikeEditor\\ClikeEditorServiceProvider"
            ]
        }
    }
}